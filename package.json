  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development -- nest start",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:debug": "dotenv -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.development -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.tests -- jest --config jest.config.e2e.ts",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name \"$(date +%s)\""
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^5.5.2",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.0",
    "dotenv-cli": "^7.3.0",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "testcontainers": "^10.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@stryker-mutator/core": "^7.3.0",
    "@stryker-mutator/jest-runner": "^7.3.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.9",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport": "^1.0.14",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^5.5.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "schema": "src/infrastructure/persistence/prisma/schema.prisma"
  }
}
